<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= t("login.email") %><br>
  <%= f.email_field :email %><br>
  <%= t("login.password") %><br>
  <%= f.password_field :password %><br>
  <%= f.submit "Log in" %>
<% end %>
<img src="/assets/i_clone_image.png">
